<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.history.HistoryActivity">

    <include
        android:id="@+id/include_app_bar_middle"
        layout="@layout/include_app_bar_middle"/>

    <LinearLayout
        android:id="@+id/container_history1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginHorizontal="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/include_app_bar_middle">

        <include
            android:id="@+id/include_history1"
            layout="@layout/item_list_history"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/container_history2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginHorizontal="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/container_history1">

        <include
            android:id="@+id/include_history2"
            layout="@layout/item_list_history"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/container_history3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginHorizontal="28dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/container_history2">

        <include
            android:id="@+id/include_history3"
            layout="@layout/item_list_history"/>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>